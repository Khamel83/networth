{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@4.3.1"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/privacy",
      "dest": "/privacy.html"
    },
    {
      "src": "/rules",
      "dest": "/rules.html"
    },
    {
      "src": "/support",
      "dest": "/support.html"
    }
  ]
}
